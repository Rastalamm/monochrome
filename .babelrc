{
  "env": {
    "es5": {
      "presets": [
        ["@babel/env", {
          "forceAllTransforms": true,
          "modules": "commonjs"
        }],
        "@babel/flow",
        "@babel/react"
      ],
      "plugins": [
        "@babel/proposal-class-properties"
      ]
    },
    "esm": {
      "presets": [
        [ "@babel/env", {
          "targets": {
            "chrome": "60",
            "edge": "15",
            "firefox": "53",
            "ios": "9",
            "safari": "9",
            "ie": "11",
            "node": "6"
          },
          "modules": false
        }],
        "@babel/flow",
        "@babel/react"
      ],
      "plugins": [
        "@babel/proposal-class-properties"
      ]
    },
    "es6": {
      "presets": [
        [ "@babel/env", {
          "targets": {
            "chrome": "60",
            "edge": "15",
            "firefox": "53",
            "ios": "10.3",
            "safari": "10.1",
            "node": "8"
          },
          "modules": false
        }],
        "@babel/flow",
        "@babel/react"
      ],
      "plugins": [
        "@babel/proposal-class-properties"
      ]
    },
    "test": {
      "presets": [
        ["@babel/env", {
          "modules": "commonjs"
        }],
        "@babel/flow",
        "@babel/react"
      ],
      "plugins": [
        "@babel/proposal-class-properties",
        "istanbul"
      ]
    }
  }
}
